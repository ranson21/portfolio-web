  "main": "src/index.js",
  "scripts": {
    "test": "npm test",
    "setup": "source setup.sh",
    "analyze": "webpack --mode=production --config webpack/webpack.prod.js --profile --json > stats.json && webpack-bundle-analyzer stats.json build",
    "start": "webpack serve --hot --mode=development --config webpack/webpack.dev.js",
    "build": "NODE_ENV=production ./config/build.sh",
    "webpack:dev": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --port=9060 --env.stats=minimal",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --host 127.0.0.1 "
