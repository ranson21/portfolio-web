{
  "name": "portfolio-web",
  "version": "1.0.0",
  "description": "Portfolio Web Application",
  "main": "src/index.js",
  "scripts": {
    "test": "npm test",
    "setup": "source setup.sh",
    "analyze": "webpack -p --mode=production --config webpack/webpack.prod.js --profile --json > stats.json && webpack-bundle-analyzer stats.json build",
    "start": "source .env && npm run webpack:dev",
    "build": "API_KEY=$API_KEY SENDER_ID=$SENDER_ID APP_ID=$APP_ID NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "docker": "docker run -d -p 80:80 portfolio-web",
    "docker:build": "docker build -t portfolio-web .",
    "webpack:dev": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --port=9060 --env.stats=minimal",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --host 127.0.0.1 "
  },
